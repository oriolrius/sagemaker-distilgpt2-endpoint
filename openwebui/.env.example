# OpenWebUI Configuration
# Copy this file to .env and update the values

# API Gateway endpoint URL (required)
# Get this from CloudFormation outputs or deploy-full-stack.sh output
OPENAI_API_BASE_URL=https://abc123.execute-api.eu-north-1.amazonaws.com

# API Key (not required for this setup, but OpenWebUI needs a value)
OPENAI_API_KEY=not-required

# OpenWebUI port (default: 80)
OPENWEBUI_PORT=80

# Enable authentication (set to true for production)
WEBUI_AUTH=false
